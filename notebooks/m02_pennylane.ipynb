   "source": [
    "@qml.qnode(device)\n",
    "def test(angle):\n",
    "    qml.RY(angle, wires=0)\n",
    "    return qml.expval(qml.PauliZ(0) @ qml.PauliZ(1))\n",
    "\n",
    "print(test(np.deg2rad(45)))"
   ]
  },
